<!DOCTYPE html>
<html>
<head>
  <title>HuskyNOM</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>

  <script type="text/javascript" src="http://widget.cloudinary.com/global/all.js"></script>

  <script type="text/javascript">
    window.API_KEYS = {
      GMAP_KEY: "<%= ENV['GMAP_KEY'] %>",
      CLOUD_NAME: "<%= ENV['CLOUD_NAME'] %>",
      UPLOAD_PRESET: "<%= ENV['UPLOAD_PRESET'] %>"
    };
  </script>

  <link href="http://netdna.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet">
<!-- <link href="path/to/css/star-rating.min.css" media="all" rel="stylesheet" type="text/css" /> -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<!-- <script src="path/to/js/star-rating.min.js" type="text/javascript"></script> -->

  <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['GMAP_KEY']}"%>
</head>
<body>

<%= yield %>

<%= javascript_include_tag 'application' %>
</body>
</html>
