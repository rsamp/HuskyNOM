<!DOCTYPE html>
<html>
<head>
  <title>HuskyNOM</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'HuskyNOM.ico' %>
</head>
<body>

<%= yield %>

<script type="text/javascript" src="http://widget.cloudinary.com/global/all.js"></script>

<script type="text/javascript">
  window.API_KEYS = {
    GMAP_KEY: "<%= ENV['GMAP_KEY'] %>"
  };
  window.CLOUDINARY_OPTIONS = {
    cloud_name: "<%= ENV['CLOUD_NAME'] %>",
    upload_preset: "<%= ENV['UPLOAD_PRESET'] %>",
    theme: 'minimal'
  };
</script>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>

<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{ENV['GMAP_KEY']}"%>
<%= javascript_include_tag 'application' %>
</body>
</html>
